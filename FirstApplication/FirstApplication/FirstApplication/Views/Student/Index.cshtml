@model List<Students>

<div class="d-flex justify-content-center" style="margin-top: 200px;">
    <table class="table table-striped table-bordered text-center w-50 shadow-sm">
        <thead class="table-dark">
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Age</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var i in Model)
            {
                <tr>
                    <td>@i.Id</td>
                    <td>@i.Name</td>
                    <td>@i.Email</td>
                    <td>@i.Age</td>
                </tr>
            }
        </tbody>
    </table>
</div>
